{
    "env": {
        "amd": true,
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 9
    },
    "rules": {
        "no-console": "off",
        "semi": ["error", "always"],
        "indent": ["error", 4],
        "no-multi-spaces": "error",
        "no-multiple-empty-lines": "error",
        "no-trailing-spaces": "error",
        "quotes": ["warn", "single"]
    }
}